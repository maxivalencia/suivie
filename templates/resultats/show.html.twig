{% extends 'suivies/index.html.twig' %}

{% block title %}Resultats{% endblock %}

{% block contenue %}
    <div class="container">
        <div class="text-center">
            <h1>Resultats</h1>
        </div>

        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ resultat.id }}</td>
                </tr>
                <tr>
                    <th>Resultat</th>
                    <td>{{ resultat.resultat }}</td>
                </tr>
            </tbody>
        </table>
        <br>
        <a href="{{ path('resultats_index') }}" class="btn btn-primary">Retour</a>
        <br><br>
        <a href="{{ path('resultats_edit', {'id': resultat.id}) }}" class="btn btn-warning">Modifier</a>
        <br><br>
        {{ include('resultats/_delete_form.html.twig') }}
    </div>
{% endblock %}
